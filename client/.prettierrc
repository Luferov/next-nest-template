{
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^@/components/(.*)$",
        "^@/ui/(.*)$",
        "^@/hooks/(.*)$",
        "^@/services/(.*)$",
        "^@/assets/(.*)$",
        "^@/utils/(.*)$",
        "^@/store/(.*)$",
        "^[./]",
        "(.scss)$" 
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}